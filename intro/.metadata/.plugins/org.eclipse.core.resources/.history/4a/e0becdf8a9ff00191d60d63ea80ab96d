package fr.afcepf.al34.demo.exeptions;

public class ExempleExeptions {

	public static void main(String[] args) {
		System.out.println("DÃ©but");
		plop();
		System.out.println("Fin");
	}
	
	public static void plop() {
		plip(0);
	}
	
	public static int plip(int x) {
		int result = 42 / x;
		return result;
	}

}
