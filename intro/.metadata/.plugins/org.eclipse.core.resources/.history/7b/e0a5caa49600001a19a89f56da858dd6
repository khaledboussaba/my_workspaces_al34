package fr.afcepf.al34;

public class Plane extends Vehicle implements IFlyable {

	@Override
	public boolean isTerrestrial() {
		return true;
	}

	public void fly() {
		System.out.println("I'm a plane and I fly !!!");
	}
	
	@Override
	public String toString() {
		return "AVION : " + super.toString();
	}

}
